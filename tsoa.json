{
    "entryFile": "src/index.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "spec": {
        "outputDirectory": "public",
        "specVersion": 3,
        "securityDefinitions": {
            "api_key": {
                "type": "http",
                "name": "access_token",
                "in": "headers",
                "scheme": "Bearer"
            }
        }
    },
    "routes": {
        "routesDir": "./temp",
        "authenticationModule": ""
    }
}